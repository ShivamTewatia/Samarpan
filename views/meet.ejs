    <script>
        // Slider functionality
        const sliders = {};
        
        function initAllSliders() {
            document.querySelectorAll('.image-slider').forEach(slider => {
                const sliderId = slider.id;
                initSlider(sliderId);
            });
        }
        
        function initSlider(sliderId) {
            const slider = document.getElementById(sliderId);
            if (!slider) return;
            
            const container = slider.querySelector('.slider-container');
            const images = slider.querySelectorAll('.slider-image');
            const dotsContainer = slider.querySelector('.slider-dots');
            
            let currentIndex = 0;
            const totalImages = images.length;
            
            // Store slider state
            sliders[sliderId] = {
                currentIndex,
                totalImages,
                container,
                images
            };
            
            // Create dots if they don't exist
            if (dotsContainer && dotsContainer.children.length === 0) {
                dotsContainer.innerHTML = '';
                for (let i = 0; i < totalImages; i++) {
                    const dot = document.createElement('span');
                    dot.classList.add('dot');
                    if (i === 0) dot.classList.add('active');
                    dot.addEventListener('click', () => goToSlide(sliderId, i));
                    dotsContainer.appendChild(dot);
                }
            }
            
            updateSlider(sliderId);
        }
        
        function updateSlider(sliderId) {
            const slider = sliders[sliderId];
            if (!slider) return;
            
            slider.container.style.transform = `translateX(-${slider.currentIndex * 100}%)`;
            
            const dotsContainer = document.querySelector(`#${sliderId} .slider-dots`);
            if (dotsContainer) {
                const dots = dotsContainer.querySelectorAll('.dot');
                dots.forEach((dot, index) => {
                    if (index === slider.currentIndex) {
                        dot.classList.add('active');
                    } else {
                        dot.classList.remove('active');
                    }
                });
            }
        }
        
        function goToSlide(sliderId, index) {
            const slider = sliders[sliderId];
            if (!slider) return;
            
            slider.currentIndex = index;
            updateSlider(sliderId);
        }
        
        function nextSlide(sliderId) {
            const slider = sliders[sliderId];
            if (!slider) return;
            
            slider.currentIndex = (slider.currentIndex + 1) % slider.totalImages;
            updateSlider(sliderId);
        }
        
        function prevSlide(sliderId) {
            const slider = sliders[sliderId];
            if (!slider) return;
            
            slider.currentIndex = (slider.currentIndex - 1 + slider.totalImages) % slider.totalImages;
            updateSlider(sliderId);
        }
        
        // Toggle sections
        function toggleSection(sectionId) {
            const section = document.getElementById(sectionId);
            section.classList.toggle('show');
            
            // Initialize sliders when section is shown
            if (section.classList.contains('show')) {
                section.querySelectorAll('.image-slider').forEach(slider => {
                    initSlider(slider.id);
                });
            }
        }
        
        // Initialize sliders when page loads
        document.addEventListener('DOMContentLoaded', function() {
            initAllSliders();
            
            // Add touch event listeners for mobile swipe
            document.querySelectorAll('.slider-container').forEach(container => {
                let touchStartX = 0;
                let touchEndX = 0;
                
                container.addEventListener('touchstart', (e) => {
                    touchStartX = e.changedTouches[0].screenX;
                }, { passive: true });
                
                container.addEventListener('touchend', (e) => {
                    touchEndX = e.changedTouches[0].screenX;
                    handleSwipe(container, touchStartX, touchEndX);
                }, { passive: true });
            });
        });
        
        function handleSwipe(container, startX, endX) {
            const sliderId = container.closest('.image-slider').id;
            if (endX < startX - 50) {
                nextSlide(sliderId);
            } else if (endX > startX + 50) {
                prevSlide(sliderId);
            }
        }
    </script>
